@model ArandanoIRT.Web._1_Application.DTOs.Admin.PlantCreateDto
@{
    ViewData["Title"] = "Registrar Nueva Planta";
    Layout = "_AdminLayout";
}
<h1>@ViewData["Title"]</h1>
<hr />
<div class="row">
    <div class="col-md-6">
        <form asp-action="Create">
            <partial name="_PlantFormPartial" model="Model" />
            <div class="form-group mt-3">
                <input type="submit" value="Registrar Planta" class="btn btn-success" />
                <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
            </div>
        </form>
    </div>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}